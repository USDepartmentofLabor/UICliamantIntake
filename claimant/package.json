    "dayjs": "^1.10.7",
    "formik": "^2.2.9",
    "i18next": "^21.6.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "launchdarkly-react-client-sdk": "^2.25.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-aria-live": "^2.0.5",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.4",
    "react-modal": "^3.14.4",
    "react-query": "^3.34.14",
    "react-router": "6.2.1",
