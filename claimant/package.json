    "@faker-js/faker": "^6.0.0-alpha.6",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.18",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "cypress": "^9.4.1",
    "cypress-audit": "^1.1.0",
    "eslint": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "http-proxy-middleware": "^2.0.1",
    "msw": "^0.36.8",
    "nyc-config-tsx": "^0.1.0",
    "pa11y": "^6.1.0",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2",
    "retire": "^3.0.3",
    "sass": "1.49.7",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0"
  },
  "resolutions": {
    "coa": "2.0.2",
    "rc": "1.2.8"
  },
  "proxy": "https://sandbox.ui.dol.gov:4430",
  "scripts": {
    "test:clean": "rm -rf .nyc_output coverage* reports",
    "nyc:clean": "rm -rf .nyc_output",
    "cypress:reconcile": "node src/reconcileCypressCoverage.js && cp coverage-cypress/coverage-final.json .nyc_output/out.json",
    "cypress:run": "yarn nyc:clean && ./node_modules/.bin/cypress run --headless --browser chrome",
    "cypress:run:dev": "yarn nyc:clean && ./node_modules/.bin/cypress run --headless --browser chrome --config-file cypress-dev.json",
    "coverage": "yarn test --coverage --watchAll=false",
    "coverage:check": "node src/coverageCheck.js",
    "test:coverage": "yarn coverage && yarn cypress:run && npx nyc report && yarn cypress:reconcile && npx nyc report",
    "pretest:coverage": "yarn nyc:clean",
    "posttest:coverage": "yarn report:combined && yarn coverage:check",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "yarn mkdir:reports",
    "copy:reports": "cp coverage-cypress/coverage-final.json reports/from-cypress.json && cp coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "yarn copy:reports && mkdir .nyc_output || true",
    "combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "yarn combine:reports",
    "report:combined": "mkdir coverage-all || true && npx nyc report --reporter html --reporter lcov --reporter text --reporter json-summary --report-dir coverage-all",
    "test:ci": "yarn test:coverage",
    "pretest:ci": "yarn nyc:clean",
    "posttest:ci": "yarn report:ci && yarn coverage:check",
    "copy:ci": "yarn copy:reports",
    "prereport:ci": "yarn combine:reports",
    "report:ci": "mkdir coverage-all || true && npx nyc report --reporter text --reporter json-summary --report-dir coverage-all",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build:instrumented": "CYPRESS_INSTRUMENT_PRODUCTION=true react-scripts -r @cypress/instrument-cra build",
    "build": "react-scripts build",
    "test": "react-scripts test --testURL https://sandbox.ui.dol.gov/claimant",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "resetMocks": false,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverageCheck.js",
      "!**/reconcileCypressCoverage.js",
      "!**/setupProxy.tsx",
      "!**/*.d.ts",
      "!src/**/*.stories.{jsx,tsx}",
      "!src/stories/**",
      "!src/i18n/**"
    ]
  },
  "nyc": {
    "extends": "nyc-config-tsx",
    "all": true,
    "report-dir": "coverage-cypress",
    "exclude": [
      "*.js",
      "**/build/**",
      "**/coverage**/**",
      "**/cypress/**",
      "**/*test.*",
      "**/*stories*",
      "**/node_modules/**",
      "**/coverageCheck.js",
      "**/reconcileCypressCoverage.js",
      "**/setupProxy.tsx",
      "**/setupTests.ts",
      "**/*.d.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
