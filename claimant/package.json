{
  "name": "claimant-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/instrument-cra": "^1.4.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@trussworks/react-uswds": "^2.4.1",
    "axios": "0.24.0",
    "formik": "^2.2.9",
    "i18next": "^21.5.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.2",
    "react-query": "^3.33.4",
    "react-router-dom": "6.0.2",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "universal-cookie": "^4.0.4",
    "uswds": "2.12.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cypress": "^9.0.0",
    "cypress-audit": "^1.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.27.0",
    "http-proxy-middleware": "^2.0.1",
    "msw": "^0.35.0",
    "pa11y": "^6.1.0",
    "prettier": "2.4.1",
    "retire": "^3.0.3",
    "sass": "1.26.3",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "babel-loader": "8.1.0",
    "coa": "2.0.2",
    "rc": "1.2.8"
  },
  "proxy": "https://sandbox.ui.dol.gov:4430",
  "scripts": {
    "cypress:run": "rm -rf .nyc_output && ./node_modules/.bin/cypress run --headless --browser chrome",
    "cypress:run:dev": "rm -rf .nyc_output && ./node_modules/.bin/cypress run --headless --browser chrome --config-file cypress-dev.json",
    "coverage": "yarn test --coverage --watchAll=false",
    "test:coverage": "yarn coverage && yarn cypress:run",
    "pretest:coverage": "rm -rf .nyc_output",
    "posttest:coverage": "yarn report:combined && node src/coverageCheck.js",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "yarn mkdir:reports",
    "copy:reports": "cp coverage-cypress/coverage-final.json reports/from-cypress.json && cp coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "yarn copy:reports && mkdir .nyc_output || true",
    "combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "yarn combine:reports",
    "report:combined": "mkdir coverage-all || true && npx nyc report --reporter lcov --reporter text --reporter json-summary --report-dir coverage-all",
    "test:ci": "yarn coverage && yarn cypress:run",
    "pretest:ci": "rm -rf .nyc_output",
    "posttest:ci": "yarn report:ci && node src/coverageCheck.js",
    "precopy:ci": "yarn mkdir:reports",
    "copy:ci": "cp coverage-cypress/coverage-final.json reports/from-cypress.json && cp coverage/coverage-final.json reports/from-jest.json",
    "precombine:ci": "yarn copy:ci && mkdir .nyc_output || true",
    "combine:ci": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:ci": "yarn combine:ci",
    "report:ci": "mkdir coverage-all || true && npx nyc report --reporter text --reporter json-summary --report-dir coverage-all",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build:instrumented": "CYPRESS_INSTRUMENT_PRODUCTION=true react-scripts -r @cypress/instrument-cra build",
    "build": "react-scripts build",
    "test": "react-scripts test --testURL https://sandbox.ui.dol.gov/claimant",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "resetMocks": false,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverageCheck.js",
      "!**/setupProxy.tsx",
      "!**/*.d.ts",
      "!src/**/*.stories.{jsx,tsx}",
      "!src/stories/**",
      "!src/i18n/**"
    ]
  },
  "nyc": {
    "report-dir": "coverage-cypress",
    "exclude": [
      "**/node_modules/**",
      "**/coverageCheck.js",
      "**/setupProxy.tsx",
      "**/react-app-env.d.ts",
      "**/types.d.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
