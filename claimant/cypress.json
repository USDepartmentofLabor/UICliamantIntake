{
  "baseUrl": "https://sandbox.ui.dol.gov:4430",
  "env": {
    "codeCoverage": {
      "url": "https://sandbox.ui.dol.gov:4430/claimant/__coverage__"
    }
  },
  "blockHosts": ["*google-analytics.com", "*googletagmanager.com"],
  "integrationFolder": "cypress/integration/tests",
  "ignoreTestFiles": "**/examples/*.spec.js",
  "screenshotOnRunFailure": false,
  "video": false,
  "defaultCommandTimeout": 5000,
  "retries": {
    "runMode": 1,
    "openMode": 1
  },
  "viewportWidth": 1920,
  "viewportHeight": 1080
}
